{
  "name": "msp-pbx-dashboard",
  "version": "2.0.0",
  "description": "Production-ready PBX monitoring dashboard for MSPs",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "logs:error": "tail -f logs/error.log",
    "logs:combined": "tail -f logs/combined.log",
    "setup:production": "node scripts/setup-production.js",
    "migrate:database": "node scripts/migrate-to-database.js",
    "backup:data": "node scripts/backup-data.js",
    "migrate:encrypt": "node scripts/encrypt-existing-data.js"
  },
  "keywords": [
    "pbx",
    "monitoring",
    "yeastar",
    "msp",
    "dashboard",
    "voip"
  ],
  "author": "Your Company",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^9.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^3.0.3",
    "socket.io": "^4.8.3",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
